html, body {color:#222;font-family:Microsoft YaHei,Helvitica,Verdana,Tohoma,Arial,san-serif;margin:0;padding: 0;text-decoration: none;}
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td {margin:0;padding:0;}
img {border:0;}
ol, ul {list-style: none outside none;margin:0;padding: 0;}
a {color:#000000;text-decoration: none;}
.body{max-width: 640px; margin: auto;}
.clr {display:block;clear:both;height:0;overflow:hidden;}
.slide {width:100%;height:220px;background:#FAFAFA;overflow:hidden;}
.slide a img {height: 220px;}

#logo {padding: 20px 0 0 20px;position: absolute;z-index: 2;}
#logo img {border-radius: 40px;}
#logo p {background-color:white;background-color:rgba(255,255,255,0.5);color:#444;padding: 4px;border-radius: 3px;line-height: 20px;font: bold 16px/22px arial,sans-serif;}
#logo span {color:white;font-size: 12px;}
#vip{position: absolute;z-index: 2;right: 8px;}		
.bdsc {width: 160px;margin: auto;}
.bkcolor{ padding: 5px 20px; border: 2px rgb(255, 108, 0); line-height: 24px; white-space: normal; color: rgb(255,255,255); text-align: center; font-weight: 800; width: 130px; border-top-left-radius: 12px; border-top-right-radius: 12px;word-wrap: break-word !important; box-sizing: border-box !important; background-color: rgb(255, 108, 0);}
.bkcontent{padding: 10px; border: 1px solid rgb(255, 108, 0); line-height: 24px; white-space: normal; font-size: 12px; color: rgb(62, 62, 62); border-top-left-radius: 0px; border-top-right-radius: 8px;  border-bottom-left-radius: 12px; border-bottom-right-radius: 12px; word-wrap: break-word !important; box-sizing: border-box !important; background-color: rgb(255, 255, 255);}
.bkcontent p{text-indent: 2em;  color: rgb(102, 102, 102); }
.copyright{text-align:center;font-size:14px;margin:20px 0px;border-top: 1px solid #ffffff;padding-top: 10px;color: #ffffff}
.copyright a{color: #ffffff}
#CustomerChatFloat{position: fixed; right: 10px; bottom: 10px; z-index: 99999; height: 40px; width: 35px; min-width: 35px; background-image: url(images/MobileChatFloat.png); background-size: 35px; background-position: 0px 0px; background-repeat: no-repeat no-repeat;}
body { background-color:#dad8d9; font-size:14px; }
body  .mainmenu {  overflow:hidden; margin:8px 8px 0px 0px; display: block;}
body  .mainmenu li { float:left;  width:33.3%;}
body  .mainmenu li a { display:block; background-color:#ff6c00;height:90px;  color:#FFF; margin:0px 0px 8px 8px;  text-align:center;  border-radius:5px ;overflow:hidden; position:relative;}
body  .mainmenu li a:active{opacity:0.5;}
body  .mainmenu li a img {  width:35px; height:35px; margin-top:15px;   }
body  .mainmenu li a p:first-of-type { display:block;  margin:auto; text-align:center;  line-height:25px; font-size:14px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap}
body  .mainmenu li a p:last-child { display: none;  margin:auto; text-align:center; height:32px; line-height:14px; font-size:12px; overflow:hidden;overflow:hidden; text-overflow:ellipsis; white-space:nowrap }
body  .mainmenu li:nth-child(10n+1) a{padding:0px;  background-color:#2b87ee;}
body  .mainmenu li:nth-child(10n+1) a p:first-of-type,body  .mainmenu li:nth-child(10n+2) a p:first-of-type{ text-align:center; }
body  .mainmenu li:nth-child(10n+1) a  p:last-child{ display:none;  }
body  .mainmenu li:nth-child(10n+3) a ,body  .mainmenu li:nth-child(10n+8) a,body  .mainmenu li:nth-child(10n+10) a{background-color:#00ccff;}
body  .mainmenu li:nth-child(10n+4) a{background-color:#2b87ee;}
body  .mainmenu li:nth-child(10n+4) p:first-of-type{ display:block;}
body  .mainmenu li:nth-child(10n+4) p:last-child{ display:none;}
body  .mainmenu li:nth-child(10n+5),body  .mainmenu li:nth-child(10n+7){width:66.6%;}
body  .mainmenu li:nth-child(10n+7) a p:first-of-type{ display:block;}
body  .mainmenu li:nth-child(10n+5) img,body  .mainmenu li:nth-child(10n+7) img{ margin-top:0px;width: auto;height: 60px;}
body  .mainmenu li:nth-child(10n+9),body  .mainmenu li:nth-child(10n+10){ width:33.3%; }