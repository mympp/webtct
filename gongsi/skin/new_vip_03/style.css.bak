* {word-break:break-all;font-family:Verdana,Arial;}
body{color:#000000;font-family:"微软雅黑","宋体"}
body,div,ul,li { padding: 0; margin: 0 auto; }
img{border:none;}
.a{width:100%;height:7px;background:#343a44;}
.b{width:100%;height:35px;background:#454545;}
.b .c{line-height:35px;width:1000px;font-size:15px;color:white;}
.header{width:100%;height:128px;background-color:#1f1f1f;text-align:center;}
.header .header_content{width:1000px;margin:0 auto;}
.header .header_content .logo{float:left;height:125px;}
.header .header_content .logo span{height:128px;display:inline-block;vertical-align:middle;}
.header .header_content .logo img{vertical-align:middle;float: left;}
.header .header_content .bor{width:5px;text-align:center;float:left;margin-top:30px;margin-left:15px;}
.header .header_content .header_con{width:375px;height:70px;float:left;color:#FFFFFF;line-height:130px;font-family:"华文行楷";}
.header .header_content .tel{width:275px;white;float:right;margin-top:20px;}
.header .header_content .tel span{color:#fc6b04;font-size:23px;position:relative;left:25px;top:25px;}
.nav{width:100%;height:40px;background-color:#ff6c00; }
.nav ul{width:1000px;}
.nav ul li{width:125px;float:left;list-style:none;line-height:40px;text-align:center;}
.nav ul li a{display:block;text-decoration:none;color:white;}
.nav ul li a:hover{background-color:#a84700;}
.nav ul .menu_on{background-color:#a84700;}
.back_color{width:100%;background-color:#dad8d9;height:386px;}
.show{width:1000px;}
.pro{width:1000px;height:40px;background-color:#1f1f1f;}
.pro_2{width:1000px;height:12px;background-color:#1f1f1f;}
.pro span{margin-left:15px;line-height:40px;font-size:13px;color:white;}
.pro a{color:#fff;}
.pro_1{margin:0 auto;clear:both;width:1000px;height:40px;background-color:#b5b5b5;}
.pro_1 span{line-height:40px;font-size:13px;color:black;}
.content_1{width:1000px;height:366px;margin:0 auto;	}
.content_1 div{margin-top:86px;float:left;width:25.1%;height:221px;text-align:center;padding:0 38px 0 38px;}
.content_1 div span{float:left;}
.content_1 div a{text-decoration:none;color:#178cd9;font-size:15px;}
.recommend{width:100%;height:398px;background-color:#e1e1e1;}
.recommend .title{margin:0 auto;width:1000px;height:55px;}
.recommend_img{margin:0 auto;width:1000px;height:321px;}
.recommend_img img{float:left;margin:10px 20px 13px 0;border:1px solid #bcbcbc;}
.intro{margin:0 auto;width:1000px;height:320px;background-color:white;}
.intro .title{height:58px;width:100%;background-color:white;margin-top:10px;}
.intro .title .bottom{height:12px;width:100%;background:url("bottom.png");}
.intro .title span{padding-left:20px; font-size:18px;color:#666666;}
.intro .in_content{width:100%;height:273px;}
.intro .in_content .con_font{float:left;width:40%;height:243px;text-indent:2em;font-size:12px;color:#666666;margin:0 15px 0 15px;;font-family:"Courier New", Courier, monospace;line-height:25px;overflow:hidden;
padding:0px 20px 10px 0px;;}
.intro .in_content .con_font span a{color:orange;text-decoration:none;}
.intro .in_content .con_img{float:left;width:55%;height:243px;}
.intro .in_content .con_img span{position:relative;left:0px;top:-58px;opacity:0.8;width:329px;height:287px;color:#FFFFFF;font-size:15px;}
.intro .in_content .con_img .con_img_1{float:left;width:175px;height:215px;text-align:center;}
.intro .in_content .con_img .con_img_2{float:left;width:175px;height:215px;margin:0 10px 0 10px;text-align:center;}
.intro .in_content .con_img .con_img_3{float:left;width:175px;height:215px;text-align:center;}
.intro .in_content .bottom_1{clear:both;height:12px;background:url("bottom.png");}
.news{margin:0 auto;width:1000px;height:260px;}
.news .news_1{float:left;width:480px;height:198px;}
.news .news_1 span{font-size:18px;padding-left:15px;color:#666666;}
.news .news_1 .bor_bottom{height:10px;background:url("border_bottom.jpg");}
.news .news_1 .more{float:right;font-size:8px;margin-top:8px;}
.news .news_1 .more a{text-decoration:none;color:#525252;}
.news .border{float:left;width:35px;height:198px;margin:30px 0 0 35px;background:url("border_1.jpg") no-repeat;}
.news .news_1 .news_con ul li{font-size:13px;list-style:none;color:#525252;border-bottom:dashed 1px #CCCCCC;margin-left:5px;padding-left:10px;line-height:25px;width:455px;}
.news .news_1 .news_con ul li a{text-decoration:none;color:#525252;}
.news .news_1 .news_con ul li span{float:right;color:#ababab;font-size:11px;}
.news .relation{float:left;width:422px;height:190px;background-color:#f1f1f1;margin-top:20px;padding:25px 0 0 20px;line-height:22px;}
.news .relation .message{float:left;width:230px;word-break: break-all;word-wrap: break-word;}
.news .relation .message span{float:left;font-size:12px;color:#525252;}
.news .relation .code{float:left;width:175px;height:147px;margin:10px 0 0 10px;}
.news .relation .code .code_font{float:left;width:28px;height:147px;text-align:center;font-size:16px;line-height:20px;padding-top:10px;}
.news .relation .code .code_bor{float:left;width:137px;height:147px;background-color:#FFFFFF;}
.news .relation .code .code_bor .code_img{float:left;width:120px;height:130px;background-color:#FF0000;margin:7px;}
.footer_nav{width:100%;height:35px;background-color:#000000;}
.footer_nav ul{width:530px;}
.footer_nav ul li{float:left;line-height:35px;list-style:none;}
.footer_nav ul li a{text-decoration:none;display:block;color:#FFFFFF;font-size:13px;}
.footer{width:100%;height:75px;background-color:#dad8d9;text-align:center;color:#515151;font-size:13px;line-height:30px;padding-top:10px;}
.footer ul{width:600px;text-align:center;}
.footer ul li{float:left;color:#515151;}
.footer ul li a{color:#515151;font-size:13px;}


/*列表页样式*/
/*公司简介*/
.list_intro{margin:0 auto;width:1000px;height:100%;background-color:white;clear:both;}
.list_intro .list_title,.list_recommend .list_title,.list_news .list_title,.list_relation .list_title,.list_demand .list_title,.list_link .list_title,.list_record .list_title,.list_hr .list_title{height:58px;width:100%;background-color:white;margin-top:10px;}
.list_intro .list_title .list_bottom,.list_recommend .list_title .list_bottom,.list_news .list_title .list_bottom,.list_relation .list_title .list_bottom,.list_demand .list_title .list_bottom,.list_link .list_title .list_bottom,.list_record .list_title .list_bottom,.list_hr .list_title .list_bottom{height:12px;width:100%;background:url("bottom.png");}
.list_intro .list_title span,.list_recommend .list_title span,.list_news .list_title span,.list_relation .list_title span,.list_demand .list_title span,.list_link .list_title span,.list_bottom .list_title span,.list_record .list_title span,.list_hr .list_title span{font-size:20px;color:#000000;}
.list_intro .list_in_content{width:100%;height:100%;}
.list_intro .list_in_content .list_con_font{width:1000px;height:100%;text-indent:2em;font-size:12px;color:#666666;font-family:"Courier New", Courier, monospace;line-height:35px;}
.list_intro .list_in_content .list_con_font span a{color:orange;text-decoration:none;}
.list_intro .list_in_content .list_con_font .list_con_img{float:right;width:550px;height:235px;text-align:center;}
.list_intro .list_in_content .list_con_font .list_con_img span{position:relative;left:25px;top:-65px;opacity:0.8;width:329px;height:287px;color:#FFFFFF;font-size:15px;}
.list_intro .list_in_content .list_con_font .list_con_img .list_con_img_1{float:left;width:175px;height:215px;text-align:center;}
.list_intro .list_in_content .list_con_font .list_con_img .list_con_img_2{float:left;width:175px;height:215px;text-align:center;}
.list_intro .list_in_content .list_con_font .list_con_img .list_con_img_3{float:left;width:175px;height:215px;text-align:center;}
.list_intro .list_in_content .list_bottom_1,.list_record .list_record_content .list_bottom_1{clear:both;height:12px;background:url("bottom.png");margin:20px 0 20px 0;}
.list_intro .list_in_content .list_con_font .list_con_img .bak{position:relative;left:22px;top:-33px;opacity:0.8;width:172;height:32;}
/*产品中心*/
.list_recommend{width:1000px;margin:0 auto;}
.list_recommend .list_recommend_img{margin-left:45px;}
.list_recommend .list_recommend_img .p_1{width:205px;height:205px;margin:10px 10px 0 10px;float:left;text-align:center;}
.list_recommend .list_recommend_img .p_1 img{border:1px solid #666666;}
.list_recommend .list_recommend_img .p_1 span{font-size:15px;color:#000000;}
/*分页*/
a { color:#333; text-decoration:none;}
ul { list-style:none;}
.pages {padding:20px 0 30px 0;text-align:center;font-size:12px;color:#666666;clear:both;}
.pages a:link,.pages a:visited,.pages a:active  {color:#004499;background:#FFFFFF;border:#9AAFE5 1px solid;padding:1px;text-decoration:none;}
.pages a:hover  {background:#FFFFFF;border:#2E6AB1 1px solid;color:#FF6600;padding:1px;text-decoration:none;}
.pages label {background:#FFFFFF;border:#2E6AB1 1px solid;color:#004499;padding:1px 5px 1px 5px;}
.pages span {color:red;font-weight:bold;}
.pages_inp {border:#2E6AB1 1px solid;padding:1px;width:26px;}
.pages_btn {width:26px;background:#F1F7FC;border:#2E6AB1 1px solid;color:#004499;font-weight:bold;font-size:11px;}

/*新闻中心,友情链接，服务需求*/
.list_news,.list_demand,.list_link,.list_hr{margin:0 auto;width:1000px;height:100%;}
.list_news span,.list_demand span,.list_link span,.list_hr span{font-size:18px;padding-left:15px;color:#666666;}
.list_news .list_news_con ul li,.list_demand .list_demand_con ul li,.list_link .list_link_con ul li,.list_hr .list_hr_con ul li{font-size:13px;list-style:none;color:#525252;border-bottom:dashed 1px #CCCCCC;margin-left:5px;padding-left:10px;line-height:25px;}
.list_news .list_news_con ul li a,.list_demand .list_demand_con ul li a,.list_link .list_link_con ul li a,.list_hr .list_hr_con ul li a{text-decoration:none;color:#525252;}
.list_news .list_news_con ul li span,.list_demand .list_demand_con ul li span,.list_link .list_link_con ul li span,.list_hr .list_hr_con ul li span{float:right;color:#ababab;font-size:11px;}

/*联系方式*/
.list_relation{margin:0 auto;width:1000px;}
.list_relation .list_relation_content{width:100%-40px;margin-top:20px;color:#666666;line-height:25px;padding:20px;font-size:15px;}
.list_relation .list_relation_content table tr{line-height:35px;font-size:15px;color:#444444;}
.list_relation .list_bottom{height:12px;width:100%;background:url("bottom.png");margin-bottom:20px;}
/*诚信档案*/
.list_record{width:1000px;height:100%;margin:0 auto;}
.list_record .list_record_content tr td{font-size:13px;line-height:30px;}
.list_record .list_record_content .f_b{font-size:13px;font-weight:bold;}


/*详情页样式*/
.news_main,.product_main,.details_parameter,.hr_main{width:1000px;height:100%;border:1px solid #CCCCCC;margin-bottom:20px;margin-top:20px;}
.news_main .main_title,.product_main .main_title,.details_parameter .main_title,.hr_main .main_title{height:40px;width:100%-20px;background-color:#b5b5b5;line-height:40px;color:#FFFFFF;padding-left:20px;font-size:15px;}
.main_con,.details_content{padding:20px;text-indent:2em;font-size:13px;line-height:25px;text-align:center;}
.main_con .a1{color:#999999;font-size:17px;}
.main_con .b1{width:100%-20px;text-align:left;height:100%;}
.product_main{height:430px;}
.de_img{width:350px;height:350px;float:left;margin-left:30px;}
.big_img{width:350px;height:250px;border:1px solid #999999;}
.big_img img{float:left;margin:12px 0 0 17px;}
.de_img ul li{width:100px;height:80px;border:#999999 1px solid;float:left;margin-top:15px;}
.de_img ul li:nth-child(2){margin:15px 22px 0 22px;}
.de_img ul li img{float:left;margin:4px 0 0 5px;}
.de_right{float:left;width:500px;height:350px;margin-left:50px;display:block;word-break: break-all;word-wrap: break-word;}
.de_right table{font-size:15px;}
.de_right tr{line-height:25px;text-align:left;}
.de_right .s{color:#0099FF;}
.parameter_content{padding:20px;text-indent:2em;font-size:13px;color:#666666;line-height:25px;padding-top:0px;}
.thumb:hover{
	background-color:#f6f6f6;
	border:1px dashed #b5b5b5;
}
.thumb img{
	border:1px solid #edd2c1;
	margin-top:10px;
}